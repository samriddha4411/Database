{
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "app/Templates/index.html"
        },
        {
            "src": "/listing",
            "dest": "run.py"
        },
        {
            "src": "/auth",
            "dest": "run.py"
        },
        {
            "src": "/login",
            "dest": "run.py"
        },
        {
            "src": "/signup",
            "dest": "run.py"
        },
        {
            "src": "/logout",
            "dest": "run.py"
        },
        {
            "src": "/db/(.*)",
            "dest": "run.py"
        },
        {
            "src": "/admin",
            "dest": "run.py"
        },
        {
            "src": "/admin/add_user",
            "dest": "run.py"
        },
        {
            "src": "/admin/edit_user/(.*)",
            "dest": "run.py"
        },
        {
            "src": "/admin/delete_user/(.*)",
            "dest": "run.py"
        },
        {
            "src": "/dashboard",
            "dest": "run.py"
        }
    ]
}